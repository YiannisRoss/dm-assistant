
 
 <% characterImageURLs = [] %>
 <%i = 0%>
 
 <% @characters.each do |character| %>

      <%characterImageURLs[i] = url_for(character.image) if character.image.persisted?%>
      <%i = i+1%>
 <%end%>  

 <% mapImageURLs = [] %>
 <% i = 0 %>

  <% @maps.each do |map| %>

      <%mapImageURLs[i] = url_for(map.image) if map.image.persisted?%>
      <%i = i+1%>
 <%end%>  

<%= react_component("GameTracker", { current_user: current_user, characters: @characters, maps: @maps, characterImageURLs: characterImageURLs, mapImageURLs: mapImageURLs }) %>